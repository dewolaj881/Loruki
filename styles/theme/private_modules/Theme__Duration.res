type t = [#v75 | #v100 | #v150 | #v200 | #v300 | #v500 | #v700 | #v1000]
let _75 = 75
let _100 = 100
let _150 = 150
let _200 = 200
let _300 = 300
let _500 = 500
let _700 = 700
let _1000 = 1000

let toValue = duration =>
  switch duration {
  | #v75 => _75
  | #v100 => _100
  | #v150 => _150
  | #v200 => _200
  | #v300 => _300
  | #v500 => _500
  | #v700 => _700
  | #v1000 => _1000
  }
